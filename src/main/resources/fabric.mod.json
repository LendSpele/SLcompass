{
  "schemaVersion": 1,
  "id": "slcomp",
  "version": "${version}",
  "name": "SLcompass",
  "description": "A mod for StoryLegends that allows you to see coordinates while holding a compass or map!",
  "authors": [
    "LendSpele"
  ],
  "contact": {
    "homepage": "www.storylegends.xyz"
  },
  "license": "MIT",
  "icon": "assets/slcomp/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.lend.slcomp.client.SlcompClient"
    ],
    "main": [
      "org.lend.slcomp.Slcomp"
    ],
    "modmenu": ["org.lend.slcomp.SLCompModMenu"]
  },
  "mixins": [
    "slcomp.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}